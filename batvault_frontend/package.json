{
  "name": "palisade-frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "codegen:regex": "node ../scripts/gen_anchor_regex.mjs",
    "codegen:openapi": "bash ../scripts/ensure_codegen_venv.sh",
    "codegen:sdk": "node ./scripts/gen_openapi_sdk.mjs",
    "codegen:types": "node ./scripts/gen_jsonschema_types.mjs",
    "openapi:check": "node ./scripts/openapi_check.mjs",
    "codegen:all": "npm run codegen:regex && npm run codegen:types && npm run codegen:openapi && npm run openapi:check && npm run codegen:sdk && npm run codegen:hooks && npm run env:sync",
    "codegen:sdk:offline": "CODEGEN_ALLOW_OFFLINE=1 node ./scripts/gen_openapi_sdk.mjs",
    "predev": "npm run codegen:all || true",
    "prebuild": "npm run codegen:all || true",
    "pretest": "npm run codegen:all || true",
    "dev": "vite",
    "build": "vite build",
    "build:sdk": "BV_BUILD_SDK=1 vite build --emptyOutDir", 
    "preview": "vite preview",
    "tailwind:init": "tailwindcss init -p",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:e2e": "playwright test",
    "test:coverage": "vitest run --coverage",
    "env:sync": "sh -c 'command -v python3 >/dev/null 2>&1 && py=python3 || py=python; exec \"$py\" ../scripts/sync_env_frontend.py'",
    "codegen:hooks": "OPENAPI_GATEWAY_SPEC=./openapi/gateway.json OPENAPI_MEMORY_SPEC=./openapi/memory.json orval --config src/sdk/orval.config.ts"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.0.0",
    "orval": "^6.25.0",
    "@mantine/core": "^8.1.1",
    "@mantine/hooks": "^8.1.1",
    "axios": "^1.4.0",
    "clsx": "^2.1.1",
    "cytoscape": "^3.32.1",
    "cytoscape-cose-bilkent": "^4.1.0",
    "cytoscape-dagre": "^2.5.0",
    "d3": "^7.9.0",
    "framer-motion": "^12.23.6",
    "jwt-decode": "^4.0.0",
    "libsodium-wrappers-sumo": "^0.7.13",
    "lucide-react": "^0.525.0",
    "openapi-fetch": "^0.10.6",
    "phosphor-react": "^1.4.1",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-cytoscapejs": "^2.0.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.5.3",
    "react-virtuoso": "^4.13.0",
    "remark-gfm": "^4.0.1",
    "yaml": "^2.8.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@playwright/test": "^1.42.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^24.2.1",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^1.4.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^24.0.0",
    "json-schema-to-typescript": "^13.1.1",
    "openapi-typescript": "^7.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.4.5",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0"
  }
}
